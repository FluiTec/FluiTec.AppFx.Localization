{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "FluiTec.AppFx.Localization": "Trace"
    }
  },
  "AllowedHosts": "*",
  "ServiceLocalizationOptions": {
    "DefaultCulture": "iv",
    "MissingLocalizationBehavior": "Log",
    "SupportedCultures": [
      "iv",
      "de",
      "de-DE"
    ],
    "MemoryCacheEntryOptions": {
      "SlidingExpiration": "00:15:00"
    }
  },
  "ServiceLocalizationImportOptions": {
    "UpdateableAuthors": [
      "Import-Code",
      "Import-File"
    ],
    "ImportFiles": [
      "./import/localization/localizations.json"
    ]
  }
}
